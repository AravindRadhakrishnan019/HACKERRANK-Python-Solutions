# Enter your code here. Read input from STDIN. Print output to STDOUT
N,M=input().split()
N=int(N);M=int(M)
K=1
L=3
a="WELCOME"
b=M-len(a)
for i in range(1,int((N+1)/2)):
    C=int(M-L)
    print(int(C/2)*"-",int(K)*".|.",int(C/2)*"-",sep="")
    K=K+2
    L=L+6
print(int(b/2)*"-",a,int(b/2)*"-",sep="")
for j in range(int((N+1)/2),N):
    K=K-2
    L=L-6
    C=int(M-L)
    print(int(C/2)*"-",int(K)*".|.",int(C/2)*"-",sep="")
    
    
